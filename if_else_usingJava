import java.util.*;

public class if_else {

	public static void main(String[] args) {
	System.out.print("Enter your age; ");
	Scanner sc=new Scanner(System.in);
	int age=sc.nextInt();
	if(age>18)
	{
		System.out.println("You can vote");
	}
	else
	{
		System.out.println("Sorry.... you can't vote");
	}

	}

}
